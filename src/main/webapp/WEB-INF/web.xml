<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ValidateImage</servlet-name>
    <servlet-class>cn.nong.validateCode.CaptchaImageCreateController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidateImage</servlet-name>
    <url-pattern>/image/captcha-image.do</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>UploadHandle</servlet-name>
    <servlet-class>cn.nong.load.UploadHandleServlet</servlet-class>
 </servlet>
 <servlet-mapping>
    <servlet-name>UploadHandle</servlet-name>
    <url-pattern>/servlet/UploadHandleServlet</url-pattern>
 </servlet-mapping>
  
</web-app>
